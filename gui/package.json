  "scripts": {
    "dev": "electron .",
    "dist": "electron-builder  --publish onTagOrDraft",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "main": "src/main/index.js",
  "build": {
    "appId": "com.eta.client",
    "productName": "ETA",
