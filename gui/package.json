    "mac": {
      "category": "your.app.category.type",
      "publish": [
        "github"
      ]
    },
    "win": {
      "extraFiles": [
        "backend.ico",
        "python-webinstall.exe",
        "*.whl",
        "*.bat"
      ],
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "electron-log": "^4.2.2",
    "electron-updater": "^4.3.5"
  },
  "devDependencies": {
	  "electron": "^9.1.0",
	  "electron-builder": "^22.7.0"
  },
  "resolutions": {}
